{
  "manifest_version": 2,
  "name": "Cookie & User-Agent Extractor",
  "version": "1.0",
  "author": "Adhi_Mahadi",
  "description": "Ambil semua cookie dan user-agent dari tab aktif.",
  "developer": {
    "name": "Info1944",
    "url": "https://github.com/info1944"
  },
  "permissions": [
    "tabs",
    "cookies",
    "<all_urls>"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
